    "build": "npm run clean && npm run tsc --",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "test": "jest",
    "watch": "npm run build -- -w",
    "watch-tsc": "tsc --watch",
